{
  "name": "profile",
  "version": "1.0.0",
  "description": "Server Rendered Profile",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base client/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "test:DOM": "mocha --compilers js:babel-core/register --require ./test/clientTest/DOM_test_helper.js 'test/clientTest/**/*.@(js|jsx)'",
    "test": "mocha --compilers js:babel-core/register --require ./test/serverTest/test_helper.js --recursive"
  },
  "author": "Bryan Schauerte",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.7",
    "autoprefixer-core": "^6.0.1",
    "classnames": "^2.2.5",
    "compression": "^1.6.1",
    "cssnano": "^3.7.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "grunt-postcss": "^0.8.0",
    "if-env": "^1.0.0",
    "immutable": "^3.7.6",
    "pixrem": "^3.0.1",
    "postcss": "^5.1.1",
    "postcss-loader": "^0.9.1",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "style-loader": "^0.13.1",
    "uuid-token-generator": "^0.5.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "css-loader": "^0.23.1",
    "grunt": "^1.0.1",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.8.0",
    "http-server": "^0.8.5",
    "jsdom": "^8.2.0",
    "mocha": "^2.4.5",
    "sass-loader": "^4.0.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
